{
    "version": 2,
    "builds": [
        {
          "src": "app.js",
          "use": "@now/node"
        }
      ],
    "routes": [
      {
        "src": "/api/user",
        "dest": "/api/user"
      },
      {
        "src": "/api/blogs",
        "dest": "/api/blogs"
      },
      {
        "src": "/api/admin",
        "dest": "/api/admin"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  